    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "jsdom": "^11.5.1",
    "json-schema-faker": "^0.4.6",
    "json-server": "^0.12.1",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.2",
    "numeral": "^2.0.6",
