  "scripts": {
    "clean:dist": "rimraf ./dist && mkdir dist",
    "build": "babel-node build/scripts/build.js",
    "prebuild": "npm-run-all clean:dist test lint",
    "postbuild": "babel-node build/scripts/distServer.js",
    "devserver": "babel-node build/scripts/devServer.js",
    "lint": "esw webpack.config.* src build/scripts --color",
    "lint:watch": "npm run lint -- --watch",
    "localtunnel": "lt --port=3000 --subdomain=odin",
    "mockdata": "json-server --watch data/mockData.json --port 3001",
    "premockdata": "babel-node build/scripts/mockDataGenerator",
    "securitycheck": "nsp check",
    "share": "npm-run-all --parallel devserver localtunnel",
    "start": "npm-run-all --parallel securitycheck mockdata share lint:watch test:watch",
    "prestart": "babel-node build/scripts/startMessage.js",
    "test": "mocha --reporter progress build/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch"
  }
}
